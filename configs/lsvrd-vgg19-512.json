{
  "dataset": "gqa",
  "word_dict": "cache/word_dict.json",
  "ent_dict": "cache/ent_dict.json",
  "pred_dict": "cache/pred_dict_311.json",
  "vision_model": {
    "image_height": 448,
    "image_width":448,
    "backbone": "vgg19",
    "feature_height": 14,
    "feature_width": 14,
    "feature_dim": 512,
    "pre_extract": true,
    "image_dir": "data/gqa/images",
    "cache_dir": "cache/gqa_vgg19",
    "emb_dim": 512,
    "crop_size": 7
  },
  "language_model": {
    "rnn_type": "GRU",
    "bidirectional": false,
    "tokens_length": 5,
    "word_emb_dim": 300,
    "emb_dim": 512,
    "n_layers": 1,
    "dropout": 0.0,
    "word_emb_init": "cache/word_emb_init.npy"
  },
  "loss_model": {
    "similarity_norm": 3.2,
    "margin": 0.2,
    "n_neg": 10
  },
  "train": {
    "learning_rate": 0.002,
    "learning_rate_decay": 0.8,
    "batch_size": 20,
    "triples_path": "cache/train_balanced_triples_use_none_label.pkl",
    "preload": false
  },
  "val": {
    "batch_size": 20,
    "triples_path": "cache/val_balanced_triples_use_none_label.pkl",
    "preload": false
  }
}
